<!DOCTYPE html>
<html>
<head>
  <title>Tuscon Rails<%= ": #{@title}" if @title%></title>
  <%= stylesheet_link_tag    "screen", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="<%= controller.controller_name %>">
<div id="wrapper">
  <div id="logo">
    <img src ="/images/logo.gif" alt="Tuscon Rails"
  </div>
  <div id="nav">
    <ul>
      <li id="nav_meetings">
        <%= link_to "Meetings", :controller=> "meetings"%>
      </li>
      <li id="nav_users">
       <%= link_to "Meetings", :controller=> "meetings"%>
      </li>      
    </ul>
  </div>
<div id="content">  
<%= yield :layout %>
</div>
</body>
</html>
